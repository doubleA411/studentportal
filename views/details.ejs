<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
      <h2>User Details</h2>
    <div class="row">
      <div class="col-md-4"></div>
      <div
        class="col-md-4"
        style="background-color: white; border-radius: 10px"
      >
        <br />

        <form id="signup-form">
          <div class="row">
            <div class="col-md-10" style="margin: 0% auto">
              <div class="form-group">
                <label
                  for="signup-email"
                  style="font-weight: 500; font-size: small"
                  >Email</label
                >
                <h2><%= email %></h2>
              </div>

              <div class="form-group">
                <label
                  for="signup-name"
                  style="font-weight: 500; font-size: small"
                  >Name</label
                >
                <h2><%= name %></h2>
              </div>
              <div class="form-group">
                <label
                  for="signup-user"
                  style="font-weight: 500; font-size: small"
                  >Username</label
                >
                <h2><%= username %></h2>
              </div>
              <div class="form-group">
              

              <div class="form-group">
                <label
                  for="signup-dob"
                  style="font-weight: 500; font-size: small"
                  >DoB</label
                >
                <h2><%= dob %></h2>
              </div>
              <div class="form-group">
                <label
                  for="signup-phone"
                  style="font-weight: 500; font-size: small"
                  >Phone</label
                >
                <h2><%= mobile %></h2>
              </div>
            
              <br /><br />
            </div>
          </div>
        </form>
      </div>
      <div class="col-md-4"></div>
    </div>
  </body>
</html>
